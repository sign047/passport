
.tit{
   color:#a8a8a8;
   font-size:28rpx;
   font-family:'Helvetica-Regular';
   text-indent:6rpx;
}

.title{
  color:#242424;
  font-size:60rpx;
  font-family:'PingFangSC-Medium', 'Helvetica Neue', 'Helvetica', 'STHeitiSC-Semibold', 'Arial', sans-serif;
}
.head{
padding-left:32rpx
}
.box{
    padding-top:56rpx;
}

.scroll{
    align-items:center;
    padding-top:20rpx;
}
.passport-item{
    display:flex;
    flex-direction:column;
    justify-content:center;
    box-shadow: 0 0 16rpx 6rpx #dfe9f5;
    border-radius:8rpx;
    width:684rpx;
    padding:44rpx 40rpx;
    position:relative;
    margin-bottom:28rpx;
    box-sizing:border-box;
}
.passport-item:nth-last-child(1){
   margin-bottom:0;
}

.passport-item .leftimg{
   display:block;
   width:126rpx;
   height:126rpx;
   border-radius:4rpx;
   margin-right:28rpx;
}
.passport-item .loc-icon{
    display:block;
    width:24rpx;
    height:30rpx;
}
.passport-item .info-name{
    width:144rpx;
    flex:0 0 144rpx;
    color:#a4b0c0;
    font-size:24rpx;
}

.passport-item .info-new{
    color:#485465;
    font-size:30rpx;
}
.star{
 position:absolute;
 top:-3rpx;
 right:44rpx;
 display:block;
 height:86rpx;
 width:70rpx;
}

.right .flex{

  line-height:30rpx;
}
.city{
    color:#485465;
    font-size:24rpx;
    margin-left:8rpx;
}
.time{
    background:rgba(168,180,196,.6);
    border-radius:8rpx;
    height:44rpx;
    width:220rpx;
    color:#fff;
    font-size:24rpx;
    text-align:center;
    line-height:44rpx;
    text-align:center;
}

.tip{
    color:rgb(168,180,196);
    font-size:24rpx;
    margin-bottom:48rpx;
    width:100%;
    padding:0 52rpx;
    box-sizing:border-box;
}
.tip .align{
    flex:1;
    margin-left:22rpx;
}
.tip .end{
   margin-left:0;
   margin-right:64rpx;
}
.tip .align .flexbox{
   margin-bottom:6rpx;
}
.tip icon{
    margin-right:10rpx;
}
.addBtn{
    border-radius:16rpx;
    height:90rpx;
    width:674rpx;
    margin:0 auto;
    background:#3b48ee;
    text-align:center;
    color:#fff;
    font-size:30rpx;
}
.addBtn .strong{
    font-size:60rpx;
    margin-right:6rpx;
    display:block;
    line-height:60rpx;
    vertical-align: middle;
}
.fixed{
  position:fixed;
  left:50%;
  transform:translate3d(-50%,0,0);
  bottom:20rpx;
}
.scroll-view{
  height:880rpx;
  margin-top:36rpx;
  background:#fff;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
page{
  background:#fff;
}
.viebox{
  margin-bottom:32rpx;
}
.cross{
  height:28rpx;
  width:28rpx;
  display:block;
  margin-right:12rpx;
}
.center{
  margin:18rpx 0;
}