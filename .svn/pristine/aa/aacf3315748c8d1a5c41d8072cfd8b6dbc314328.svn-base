<view class="user-wrapper">
    <view class="avatar vertical">
        <image src="{{avatarUrl}}"></image>
        <text wx-if="nickName">{{nickName}}</text>
    </view>
    <view class="flexbox between user" catchtap="toPhone">手机号<text class="phone-txt">{{phoneTxt}}</text><image  src="../../images/right.png" class="radius"></image></view>
    <view class="flexbox between user" catchtap="toFeedBack">意见反馈<image  src="../../images/right.png" class="radius"></image></view>
</view>